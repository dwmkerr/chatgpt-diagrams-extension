{
  "manifest_version": 3,
  "name": "ChatGPT Diagrams",
  "description": "Render diagrams directly in ChatGPT",
  "content_scripts": [{
    "js": ["content.ts"],
    "matches": [
      "https://chat.openai.com/*",
      "http://localhost/*"
    ]
  }],
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
